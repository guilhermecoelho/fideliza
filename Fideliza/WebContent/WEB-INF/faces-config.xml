	<converter>  
     <description>Um converter</description>  
     <converter-id>EmpresaConverter</converter-id>  
     	<converter-class>  
     	br.com.fideliza.converter.EmpresaConverter
     	</converter-class>
     </converter>
	<lifecycle>
		<phase-listener>br.com.fideliza.filter.AuthorizationListener</phase-listener>
	</lifecycle>

</faces-config>
