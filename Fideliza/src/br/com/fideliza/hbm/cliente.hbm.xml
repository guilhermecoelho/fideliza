<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="br.com.fideliza.model.Cliente" table="CLIENTE">


		<id name="idCliente" column="ID_CLIENTE" >
		    <generator class="increment"/>
		</id>

		<property name="nome" column="NOME_CLIENTE" />
		<property name="password" column="PASSWORD_CLIENTE" />
		<property name="email" column="EMAIL_CLIENTE" />
		<property name="cpf" column="CPF_CLIENTE" />

	</class>

</hibernate-mapping>
