<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="br.com.fideliza.model.Usuario" table="usuario">


		<id name="idUser" column="idUser" >
		    <generator class="increment"/>
		</id>

		<property name="user" column="nomeUser" />
		<property name="password" column="senhaUser" />
		<property name="email" column="emailUser" />
		<property name="cidade" column="cidadeUser" />
		<property name="cpf2" column="cpfUser2" />

	</class>

</hibernate-mapping>
