<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="br.com.fideliza.model.Usuario" table="usuario">


		<id name="idUser" column="idUser" />

		<property name="user" column="nomeUser" />
		<property name="password" column="senhaUser" />


	</class>

</hibernate-mapping>
