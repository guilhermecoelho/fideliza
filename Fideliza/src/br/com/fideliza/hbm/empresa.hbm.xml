<?xml version="1.0"?>

<!DOCTYPE hibernate-mapping PUBLIC

   "-//Hibernate/Hibernate Mapping DTD 3.0//EN"

   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="br.com.fideliza.model.Empresa" table="EMPRESA">


		<id name="idEmpresa" column="ID_EMPRESA" >
		    <generator class="increment"/>
		</id>

		<property name="nome" column="NOME_EMPRESA" />
		<property name="password" column="PASSWORD_EMPRESA" />
		<property name="email" column="EMAIL_EMPRESA" />
		<property name="cnpj" column="CNPJ_EMPRESA" />

	</class>

</hibernate-mapping>
